<h1 class="page-header">Users</h1>
<% if (typeof users != 'undefined') { %>
<%
	var limit = 10;
	var pages = Math.ceil(numUsers/limit);
	var currentPage = (1/limit) + 1;
	var next = 1 + limit; 
	var back = 1 - limit;
%>
<% if (numUsers > 10) { %>
	<nav>
	  <ul class="pagination">
		<% for (var i = 1; i <= pages; i++) { 
			var skip = (i-1)*limit; 
			var active = i == currentPage; %>
			<li <%= active ? "class=active" : "" %>><a href="/admin/feedback?offset=<%=skip%>"><%= i %></a></li>
		<% } %>
	  </ul>
	</nav>
<% } %>
<table class="table table-striped table-bordered" id="resultsTable">
	<thead>
		<tr>
			<th>User ID</th>
			<th>Username</th>
			<th>Date</th>
		</tr>
	</thead>
	<tbody>
	<% _.each(users, function (u) { %>
		<tr>
			<td><a href="/admin/feedback/equation?equationID=<%= u.id %>"><%= u.id %></a></td>
			<td><%= u.username %></td>
			<td><%= u.createdAt %></td>
		</tr>
	<% }) %>
	</tbody>
</table>
<% } else { %>
<p class="text-info">No users.</p>
<% } %>