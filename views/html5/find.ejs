<h2>HTML5 Upload</h2>
<% if (typeof html5 != 'undefined') { %>
	<p><strong>Filename:</strong> <%= html5.filename %></p>
	<p><strong>Output Format:</strong> <%= html5.outputFormat %></p>
	<p id="download" style="display: none;"><strong>Download:</strong>
		<a href="/html5/download/<%= html5.id %>?source=true"><span class="sr-only">Download</span> <%= html5.filename %> source</a> | 
		<a href="/html5/<%= html5.id %>/output"><span class="sr-only">Download</span> <%= html5.filename %> output</a>
	</p>
	<div id="processing">
		<img src="/images/load.gif" alt="Please wait" />
	</div>
	<div id="equations">
	<% if (typeof equations != 'undefined' && equations.length > 0) { %>
		<%- partial ('equations.ejs') %> 
	<% } %>
	</div>
	
<% } %>
